msgid "Firefox Live"
msgstr "فَيَرفُكس لايف"

#: ../mozilla.com-translations/ar/firefoxlive.lang:6
msgid "Tweet"
msgstr "تويت"

#: ../mozilla.com-translations/ar/firefoxlive.lang:10
msgid "Follow @cubcaretaker"
msgstr "تابِع @cubcaretaker"

#: ../mozilla.com-translations/ar/firefoxlive.lang:14
msgid "Cameras"
msgstr "كاميرا"

#: ../mozilla.com-translations/ar/firefoxlive.lang:18
msgid "360° view"
msgstr "‫عرض 360°"

#: ../mozilla.com-translations/ar/firefoxlive.lang:22
msgid "Camera 1"
msgstr "كاميرا 1"

#: ../mozilla.com-translations/ar/firefoxlive.lang:26
msgid "Camera 2"
msgstr "كاميرا 2"

