#. main title
#: firefox/desktop/tips.lang:7
msgid "The best of the latest Firefox"
msgstr "Najbolje od posljednjeg Firefoxa"

#: firefox/desktop/tips.lang:11
msgid "Bookmarks"
msgstr "Zabilješke"

#: firefox/desktop/tips.lang:15
msgid "Simplify"
msgstr "Pojednostavi"

#: firefox/desktop/tips.lang:19
msgid "Arrange"
msgstr "Organizuj"

#: firefox/desktop/tips.lang:23
msgid "Sync"
msgstr "Sinhronizacija"

#: firefox/desktop/tips.lang:27
msgid "5 top tips, tricks and time-saving tools"
msgstr "5 top savjeta, trikova i alata za uštedu vremena"

#: firefox/desktop/tips.lang:31
msgid "1"
msgstr "1"

#: firefox/desktop/tips.lang:35
msgid "2"
msgstr "2"

