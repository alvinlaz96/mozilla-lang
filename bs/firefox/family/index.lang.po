#: firefox/family/index.lang:62
msgid "Firefox <span>for</span> desktop"
msgstr "Firefox <span>za</span> desktop"

#: firefox/family/index.lang:66
msgid "Trusted. Flexible. Fast."
msgstr "Pouzdan. Fleksibilan. Brz."

#. <span> tag below adds visual style to "for" text
#: firefox/family/index.lang:71
msgid "Firefox <span>for</span> Android"
msgstr "Firefox <span>za</span> Android"

#: firefox/family/index.lang:75
msgid "Make it uniquely yours."
msgstr "Neka bude jedinstveno vaš."

#: firefox/family/index.lang:79
msgid "Get it on Google Play"
msgstr "Preuzmite sa Google Play-a"

#. # TAG: tracking_protection
#. <span> tag below adds visual style to "for" text
#: firefox/family/index.lang:85
msgid "Firefox <span>for</span> iOS"
