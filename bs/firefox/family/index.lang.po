#. # TAG: ios_product_tagline
#: firefox/family/index.lang:90
msgid "Fast. Smart. Yours."
msgstr "Brz. Pametan. Vaš."

#. # TAG: tracking_protection
#. Obsolete title, do not remove
#: firefox/family/index.lang:96
msgid "Get it on iPhones and iPads"
msgstr "Nabavite ga za iPhone i iPode"

#. # TAG: tracking_protection
#: firefox/family/index.lang:101
msgid "Now with Tracking Protection."
msgstr "Sada sa zaštitom od praćenja."

#: firefox/family/index.lang:105
msgid "Connect Firefox wherever you use it."
msgstr "Povežite Firefox gdje god da ga koristite."

