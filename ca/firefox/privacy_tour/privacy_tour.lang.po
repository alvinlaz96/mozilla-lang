#: firefox/privacy_tour/privacy_tour.lang:156
msgid "%s lets you <br>search anonymously"
msgstr ""

#: firefox/privacy_tour/privacy_tour.lang:160
msgid "Get great results with less tracking"
msgstr ""

#: firefox/privacy_tour/privacy_tour.lang:164
msgid "Make it (or any other) your default"
msgstr ""

#: firefox/privacy_tour/privacy_tour.lang:168
msgid "Find what you’re looking for, your way"
msgstr ""

#. %s is a searchplugin name
#: firefox/privacy_tour/privacy_tour.lang:173
#, fuzzy
msgid "<span>Previous:</span> %s"
msgstr "<span>Anterior:</span> Menú"

#. STEP 3 of the tour (Private Browsing)
#. <br> tag below used for formatting only.
#: firefox/privacy_tour/privacy_tour.lang:179
msgid "Private Browsing: <br>Our original privacy tool"
msgstr ""

#: firefox/privacy_tour/privacy_tour.lang:183
msgid "Keep history and other info off your computer"
msgstr ""

#: firefox/privacy_tour/privacy_tour.lang:187
msgid "Browse without recording where you’ve been"
msgstr ""

#: firefox/privacy_tour/privacy_tour.lang:191
msgid "So others using your computer can’t see it"
