msgstr "Firefox Live"

#: ../mozilla.com-translations/es/firefoxlive.lang:6
msgid "Tweet"
msgstr "Tuit"

#: ../mozilla.com-translations/es/firefoxlive.lang:10
msgid "Follow @cubcaretaker"
msgstr "Sigue a @cubcaretaker"

#: ../mozilla.com-translations/es/firefoxlive.lang:14
msgid "Cameras"
msgstr "Cámaras"

#: ../mozilla.com-translations/es/firefoxlive.lang:18
msgid "360° view"
msgstr "Visión de 360°"

#: ../mozilla.com-translations/es/firefoxlive.lang:22
msgid "Camera 1"
msgstr "Cámara 1"

#: ../mozilla.com-translations/es/firefoxlive.lang:26
msgid "Camera 2"
