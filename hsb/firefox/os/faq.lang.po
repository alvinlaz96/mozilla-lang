msgstr "Venezuela z <a href=\"%s\">Movistar</a>"

#: firefox/os/faq.lang:100
msgid "Colombia with <a href=\"%s\">Movistar</a>"
msgstr "Kolumbiska z<a href=\"%s\">Movistar</a>"

#: firefox/os/faq.lang:104
msgid ""
"Hungary with both <a href=\"%s\">Telenor</a> and <a href=\"%s\">T-Mobile</a>"
msgstr "Madźarska z <a href=\"%s\">Telenor</a> a <a href=\"%s\">T-Mobile</a>"

#: firefox/os/faq.lang:108
msgid "Peru with <a href=\"%s\">Movistar</a>"
msgstr "Peru z <a href=\"%s\">Movistar</a>"

