#~ msgid "Text"
#~ msgstr "Мәтін"

#~ msgid "Whoops! Be sure to enter a valid email address."
#~ msgstr "Дұрыс эл. пошта адресін енгізіңіз."

#~ msgid "Format"
#~ msgstr "Пішім"

#~ msgid "Get Monthly News"
#~ msgstr "Ай сайынғы жаңалықтарды алу"

