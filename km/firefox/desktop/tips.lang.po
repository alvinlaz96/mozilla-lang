msgid "Bookmarks"
msgstr "ចំណាំ"

#: firefox/desktop/tips.lang:13
msgid "Simplify"
msgstr "ធ្វើ​ឲ្យ​សាមញ្ញ​"

#: firefox/desktop/tips.lang:17
msgid "Arrange"
msgstr "រៀបចំ"

#: firefox/desktop/tips.lang:21
msgid "Sync"
msgstr "ធ្វើ​សមកាលកម្ម"

#: firefox/desktop/tips.lang:25
msgid "5 top tips, tricks and time-saving tools"
msgstr ""

