msgid "Camera app"
msgstr "កម្មវិធី​ម៉ាស៊ីន​ថត"

#. image alt text
#: firefox/os/index.lang:247
msgid "Facebook app"
msgstr "កម្មវិធី Facebook"

#. image alt text
#: firefox/os/index.lang:252
msgid "Maps app"
msgstr "កម្មវិធី​ផែន​ទី"

#. image alt text
#: firefox/os/index.lang:257
msgid "Photos app"
msgstr "កម្មវិធី​រូបថត"

#. image alt text
#: firefox/os/index.lang:262
msgid "Music app"
msgstr "កម្ម​វិធី​តន្ត្រី"

